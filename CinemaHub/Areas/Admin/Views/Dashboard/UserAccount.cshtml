@model IEnumerable<AppUser>
<div class="content">
    <div class="container">
        <div class="container my-5">
            <h3 class="text-primary my-4">Manage Account</h3>
            <hr />
            <div class="my-5"></div>
            <table id="userAccountTable" class="table table-striped table-bordered my-5">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>First Name</th>
                        <th>Last Name</th>
                        <th>Email</th>
                        <th>Phone Number</th>
                        <th>Status</th>
                        <th>Point</th>
                        <th>Manage</th>
                    </tr>
                </thead>
            </table>
            <div class="d-flex w-100 my-5 justify-content-start error-actions">
                <a class="btn btn-outline-success mx-2" asp-area="Customer" asp-controller="Home" asp-action="Index">
                    <i class="bi bi-plus-circle-dotted"></i> Back to Home
                </a>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script type="text/javascript" src="~/js/userAccount.js"></script>
}